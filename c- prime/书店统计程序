#include<iostream>
#include "Sale_item.h"
using namespace std;
int main()
{
    Sales_item total;
    if(cin >> total){
        Sales_item trans;
        while(cin >> trans){
            if(total.isbn() == trans.isbn())
                total += trans;
            else {
                cout << total << endl;
                total = trans;
            }
        }
        cout << total << endl;
    }
    else{
        cerr << "No Data " << endl;
        return -1;
    }
}
